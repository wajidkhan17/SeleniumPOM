<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Opencart App Test Regression Suite" thread-count="3"
	parallel="tests" verbose="4">
	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener"></listener>
	</listeners>
	<test name="Login Page Test">
		<classes>
			<class name="com.qa.opencart.test.LoginPageTest"></class>
		</classes>
	</test>
	<test name="Account Page Test">
		<classes>
			<class name="com.qa.opencart.test.AccountPageTest"></class>
		</classes>
	</test>
	<test name="Product Page Test">
		<classes>
			<class name="com.qa.opencart.test.ProductSearchTest"></class>
		</classes>
	</test>
</suite> <!--
Opencart App Test Regression Suite -->
